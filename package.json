{
  "name": "actions/attest",
  "description": "Generate signed attestations for workflow artifacts",
  "version": "2.2.0",
  "author": "",
  "private": true,
  "homepage": "https://github.com/actions/attest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/attest.git"
  },
  "bugs": {
    "url": "https://github.com/actions/attest/issues"
  },
  "keywords": [
    "actions",
    "attestation"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "jest",
    "format:write": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint:eslint": "npx eslint . -c ./.github/linters/eslint.config.mjs",
    "lint:markdown": "npx markdownlint --config .github/linters/.markdown-lint.yml \"*.md\"",
    "lint": "npm run lint:eslint && npm run lint:markdown",
    "package": "ncc build src/index.ts --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "test": "jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run package"
  },
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "verbose": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ]
  },
  "dependencies": {
    "@actions/attest": "^1.5.0",
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@actions/glob": "^0.5.0",
    "@sigstore/oci": "^0.4.0",
    "csv-parse": "^5.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@sigstore/mock": "^0.10.0",
    "@types/jest": "^29.5.14",
    "@types/make-fetch-happen": "^10.0.4",
    "@types/node": "^22.13.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.20.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "markdownlint-cli": "^0.44.0",
    "nock": "^13.5.6",
    "prettier": "^3.5.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "undici": "^5.28.5"
  }
}
